generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  id           String  @id @default(uuid())
  cnpj         String  @unique
  razaoSocial  String
  nomeFantasia String
  telefone     String
  andar        String?
  sala         String?
}
