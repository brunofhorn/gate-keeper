generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  id           String  @id @default(uuid())
  cnpj         String  @unique
  razaoSocial  String
  nomeFantasia String
  telefone     String
  andar        String?
  sala         String?
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  username String @unique
  name     String
  cpf      String
  password String
}
